{
  "name": "ember-3d-nav",
  "version": "1.13.0",
  "description": "A 3d rotating top navigation bar for Ember apps",
  "keywords": [
    "ember-addon",
    "navbar",
    "nav",
    "3d nav",
    "navigation"
  ],
  "repository": "https://github.com/shipshapecode/ember-3d-nav",
  "license": "MIT",
  "author": {
    "name": "Robert Wagner",
    "email": "rwwagner90@gmail.com",
    "url": "https://github.com/rwwagner90"
  },
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*",
    "lint:hbs": "ember-template-lint .",
    "lint:js": "eslint .",
    "start": "ember serve",
    "test": "npm-run-all lint:* test:*",
    "test:ember": "ember test",
    "test:ember-compatibility": "ember try:each"
  },
  "dependencies": {
    "ember-cli-babel": "^7.21.0",
    "ember-cli-htmlbars": "^5.2.0",
    "ember-cli-sass": "^10.0.1",
    "ember-decorators": "^6.1.1",
    "ember-headroom": "^1.1.2",
    "ember-router-service-polyfill": "^1.0.2",
    "sass": "^1.24.0"
  },
  "devDependencies": {
    "@ember/optional-features": "^1.3.0",
    "@glimmer/component": "^1.0.1",
    "@glimmer/tracking": "^1.0.0",
    "babel-eslint": "^10.1.0",
    "broccoli-asset-rev": "^3.0.0",
    "codeclimate-test-reporter": "^0.5.0",
    "ember-auto-import": "^1.6.0",
    "ember-chrome-devtools": "^0.2.0",
    "ember-cli": "~3.20.0",
    "ember-cli-code-coverage": "1.0.0-beta.8",
    "ember-cli-dependency-checker": "^3.2.0",
    "ember-cli-deploy": "^1.0.2",
    "ember-cli-deploy-build": "^2.0.0",
    "ember-cli-deploy-ghpages": "^0.2.0",
    "ember-cli-inject-live-reload": "^2.0.2",
    "ember-cli-stylelint": "^3.0.2",
    "ember-cli-uglify": "^3.0.0",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-export-application-global": "^2.0.1",
    "ember-load-initializers": "^2.1.1",
    "ember-maybe-import-regenerator": "^0.1.6",
    "ember-native-dom-helpers": "^0.6.3",
    "ember-qunit": "^4.6.0",
    "ember-resolver": "^8.0.0",
    "ember-source": "~3.20.2",
    "ember-source-channel-url": "^2.0.1",
    "ember-template-lint": "^2.9.1",
    "ember-try": "^1.4.0",
    "eslint": "^7.5.0",
    "eslint-plugin-ember": "^8.9.1",
    "eslint-plugin-node": "^11.1.0",
    "loader.js": "^4.7.0",
    "npm-run-all": "^4.1.5",
    "qunit-dom": "^1.4.0",
    "release-it": "^13.6.5",
    "release-it-lerna-changelog": "^2.3.0",
    "stylelint-config-ship-shape": "^0.7.0"
  },
  "engines": {
    "node": "10.* || >= 12"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "demoURL": "https://shipshapecode.github.io/ember-3d-nav/"
  },
  "release-it": {
    "plugins": {
      "release-it-lerna-changelog": {
        "infile": "CHANGELOG.md",
        "launchEditor": true
      }
    },
    "git": {
      "tagName": "v${version}"
    },
    "github": {
      "release": true,
      "tokenRef": "GITHUB_AUTH"
    }
  }
}
